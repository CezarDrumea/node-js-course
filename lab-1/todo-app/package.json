{
  "name": "express-todo-mvc",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm:start:db\" \"npm:start:server\"",
    "start:db": "json-server --watch db.json --port 4000",
    "start:server": "node --watch src/server.js"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "express": "^4.19.2",
    "pug": "^3.0.3"
  },
  "devDependencies": {
    "concurrently": "^9.0.1",
    "json-server": "^1.0.0-beta.3"
  }
}
